@model Day4_MVC_lab7___sol___Ali_Ahmed.Models.Student

@{
    Layout = null;
}



<!DOCTYPE html>

<html lang="en">
<head>
    <title>Sign Up</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <!-- Font Icon -->
    <link href="~/colorlib-regform-7/fonts/material-icon/css/material-design-iconic-font.min.css" rel="stylesheet" />
    <!-- Main css -->
    <link href="~/colorlib-regform-7/css/style.css" rel="stylesheet" />

</head>
<body>
    <!-- Sign up form -->
    <section class="signup">
        <div class="container">
            <div class="signup-content">
                <div class="signup-form">
                    <h2 class="form-title">Sign up</h2>

                    @using (Html.BeginForm("Register", "ITI", FormMethod.Post))
                    {
                        <div class="form-group">
                            @Html.LabelFor(u => u.Name)
                            <br />
                            <br />
                            @Html.TextBoxFor(s => s.Name)
                            @Html.ValidationMessageFor(u => u.Name, "", htmlAttributes: new { Style = "color:red" })
                        </div>
                        <div class="form-group">
                            @Html.LabelFor(s => s.Phone)
                            <br />
                            <br />
                            @Html.TextBoxFor(s => s.Phone)
                            @Html.ValidationMessageFor(u => u.Phone, "", htmlAttributes: new { Style = "color:red" })
                        </div>
                        <div class="form-group">

                            @Html.LabelFor(s => s.Email)
                            <br />
                            <br />
                            <div class="col-md-10">

                                @Html.TextBoxFor(s => s.Email)
                                @Html.ValidationMessageFor(u => u.Email, "", htmlAttributes: new { Style = "color:red" })
                            </div>
                        </div> <div class="form-group">
                            @Html.LabelFor(s => s.Password)
                            <br />
                            <br />
                            @Html.PasswordFor(s => s.Password)
                            @Html.ValidationMessageFor(u => u.Password, "", htmlAttributes: new { Style = "color:red" })
                        </div>
                        <input type="submit" name="signup" id="signup" class="form-submit" value="Register" />
                        @*<input type="submit" value="Create" />*@
                    }


                    <!--<form method="POST" class="register-form" id="register-form" action="~/ITI/Register" autocomplete="off"--> @*role="main"*@

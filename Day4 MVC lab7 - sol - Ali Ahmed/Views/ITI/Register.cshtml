                    <!--<div class="form-group">
                        <label for="password"><i class="zmdi zmdi-lock material-icons-name"></i></label>
                        <input type="password" name="password" id="password" placeholder="Password" tabindex="4" required />
                        @Html.ValidationMessageFor(u => u.Password, "", htmlAttributes: new { Style = "color:red" })
                    </div>-->
                    <!-- #endregion -->
                    <!-- #region Terms of service -->
                    <!--@*<div class="form-group">
